{
  "expo": {
    "name": "Echopllay",
    "slug": "echopllay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "echopllay",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.PNG",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-av",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow EchoPlay to access your photos",
          "videoPermission": "Allow EchoPlay to access your videos",
          "savePhotosPermission": "Allow EchoPlay to save photos",
          "saveVideosPermission": "Allow EchoPlay to save videos"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "permissions": [
      "READ_EXTERNAL_STORAGE",
      "WRITE_EXTERNAL_STORAGE"
    ]
  }
}